{
    "name": "life-style-coding/container-pattern-php",
    "type": "library",
    "description": "Small library of dependency inversion with container",
    "keywords": ["dependency inversion", "IOT", "container","php"],
    "homepage": "https://github.com/niko-38500/container-pattern-php.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Montmayeur",
            "email": "nicolas.montmayeur@hotmail.fr"
        }
    ],
    "scripts": {
        "tests": "./vendor/bin/phpunit ./tests"
    },
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "Container\\": "",
            "exclude-from-classmap": [
                "/Tests/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Container\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
